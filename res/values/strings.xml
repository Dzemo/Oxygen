    <!-- CONFIG DEBUT -->


    <!--
         Addresse URL par défaut du serveur de synchronisation, est équivalente 
    	 à l'addresse utilisé par les utilisateurs pour accéder à l'interface web
    -->
    <string name="remote_url_default">http://oxygen.plonge.mobi</string>

    <!-- Si le login de l'utilisateur est enregistré sur l'écran de connexion, boolean (true/false) -->
    <string name="save_login_default">false</string>

    <!-- Si l'utilisateur reste connecté à l'application, boolean (true/false) -->
    <string name="keep_connection_default">false</string>

    <!--
         Si, lors de la synchronisation, toute les fiches sont récupérés ou uniquement celle dont le directeur 
    	 de plongé est associés à l'utilisateur actuelle, boolean (true/false)
    -->
    <string name="retrieve_all_type_fiche_default">false</string>

    <!-- Nombre de jours de fiche récupérs, à 0 récupère uniquement les fiches du jours actuel, max 31 -->
    <string name="retrieve_length_default">0</string>

    <!-- Profondeur par défaut (en metres) -->
    <string name="plonger_profondeur_default">40</string>

    <!-- Durée par défaut (en minute) -->
    <string name="plonger_duree_default">20</string>
    
    <!-- Nombre de plongeur suggérés parmis les derniers plongeur dans la creation d'une fiche -->
	<string name="nombre_plongeur_suggerer">15</string>
	
